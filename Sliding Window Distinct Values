#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,k; cin>>n>>k;
    vector<int>v(n);
    for(auto &x:v) cin>>x;
    map<int,int>m;
    int d=0;
    for(int i=0;i<k;i++)
    {
        if(m[v[i]]==0)
            d++;
        m[v[i]]++;
    }
    cout<<d<<" ";
    for(int i=k;i<n;i++)
    {
        m[v[i-k]]--;
        if(m[v[i-k]]==0) d--;
        if(m[v[i]]==0) d++;
        m[v[i]]++;
        cout<<d<<" ";
    }
}
