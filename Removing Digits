#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;cin>>n;
    vector<int> f(n+1,1e9);
    f[0]=0;
    for(int i=1;i<=n;i++)
    {
        int t=i;
        while(t>0)
        {
            int d=t%10;
            if(d>0) f[i]=min(f[i],f[i-d]+1);
            t/=10;
        }
    }
    cout<<f[n]<<"\n";
}
