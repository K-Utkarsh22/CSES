#include <bits/stdc++.h>
using namespace std;
int is(long long n)
{
    if(n<2) return 0;
    for(long long i=2;i*i<=n;i++)
        if(n%i==0) return 0;
    return 1;
}
int main()
{
    int n;cin>>n;
    vector<int> a(n),b(n);
    int r=n;
    while(r>0)
    {
        int p=r+1;
        while(!is(p))p++;
        int s=p-r;
        for(int i=s;i<=r;i++)
        {
            a[i-1]=i;
            b[i-1]=p-i;
        }
        r=s-1;
    }
    for(auto x:a)cout<<x<<" ";
    cout<<"\n";
    for(auto x:b)cout<<x<<" ";
}
