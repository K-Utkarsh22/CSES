#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        vector<int> v(n);
        bool f=0;
        for(auto &x:v)
        {
            cin>>x;
            if(x%2!=0)
                f=1;
        }
        if(f)
            cout<<"first"<<"\n";
        else
            cout<<"second"<<"\n";
    }
}
