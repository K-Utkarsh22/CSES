#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;cin>>n;
    long long x;cin>>x;
    vector<long long> v(n);
    for(auto &x:v) cin>>x;
    sort(v.begin(),v.end());
    long long i=0,j=n-1,c=0;
    while(i<=j)
    {
        if(v[i]+v[j]<=x)
            i++,j--;
        else j--;
        c++;
    }
}
