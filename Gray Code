#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;cin>>n;
    vector<string> v;
    v.push_back("0");
    v.push_back("1");
    for(int i=1;i<n;i++)
    {
        int s=v.size();
        for(int j=s-1;j>=0;j--) v.push_back(v[j]);
        for(int j=0;j<s;j++) v[j]="0"+v[j];
        for(int j=s;j<v.size();j++) v[j]="1"+v[j];
    }
    for(auto &x:v) cout<<x<<"\n";
}
