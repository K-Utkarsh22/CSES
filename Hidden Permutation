#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;cin>>n;
    vector<long long> a(n);
    iota(a.begin(),a.end(),1);
    stable_sort(a.begin(),a.end(),[](int i,int j)
    {
        cout<<"? "<<i<<" "<<j<<endl;
        string s;
        cin>>s;
        return s=="YES";
    });
    vector<int> r(n+1);
    for(int i=0;i<n;i++)
    {
        r[a[i]]=i+1;
    }
    cout<<"!";
    for(int i=1;i<=n;i++)
    {
        cout<<" "<<r[i];
    }
    cout<<endl;
}
