#include<bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        long long k,c=1,f=9;
        cin>>k;
        while((k-1)/c>=f)
        {
            k-=c*f;
            c++;
            f*=10;
        }
        long long p=1;
        for(int i=1;i<c;i++) p*=10;
        long long n=p+(k-1)/c,r=c-1-(k-1)%c;
        while(r--)
        {
            n/=10;
        }
        cout<<n%10<<"\n";
    }
}
