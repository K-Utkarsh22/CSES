#include <bits/stdc++.h>
using namespace std;
long long M=1e9+7;
int main()
{
    int n;
    long long x;
    cin>>n>>x;
    vector<int>v(n);
    multiset<int>s;
    for(auto &k:v)
    {
        cin>>k;
        s.insert(k);
    }
    for(int i=0;i<n;i++)
    {
        long long t=x-v[i];
        if(s.find(t)!=s.end())
        {
            if(t==v[i] && s.count(t)<2)
                continue;
            cout<<i+1<<" ";
            for(int j=0;j<n;j++)
            {
                if(v[j]==t && j!=i)
                {
                    cout<<j+1;
                    return 0;
                }
            }
        }
    }
    cout<<"IMPOSSIBLE";
}
