#include <bits/stdc++.h>
using namespace std;
int n=0;
vector<string> b(8);
vector<int> v(8,0),d(15,0),a(15,0);
void rec(int r)
{
    if(r==8)
    {
        n++;
        return;
    }
    for(int i=0;i<8;i++)
    {
        if(b[r][i]=='.' && !v[i] && !d[r-i+7] && !a[r+i])
        {
            v[i]=d[r-i+7]=a[r+i]=1;
            rec(r+1);
            v[i]=d[r-i+7]=a[r+i]=0;
        }
    }
}

int main()
{
    for(auto &s:b)cin>>s;
    rec(0);
    cout<<n<<"\n";
}
