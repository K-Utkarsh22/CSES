#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,k;cin>>n>>k;
    vector<int> v1(n),v2(n);
    for(auto &x:v1)cin>>x;
    for(auto &x:v2)cin>>x;
    vector<int> d(k+1,0);
    for(int i=0;i<n;i++)
    {
        for(int j=k;j>=v1[i];j--)
        {
            d[j]=max(d[j],d[j-v1[i]]+v2[i]);
        }
    }
    int res=d[k];
    cout<<res<<"\n";
}
