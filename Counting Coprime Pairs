#include<bits/stdc++.h>
#define ll long long
using namespace std;
int N=1000000;
vector<ll> freq(N+1),c(N+1),r(N+1);
int main()
{
    ll n;cin>>n;
    vector<int> v(n);
    for(auto &x:v)
    {
        cin>>x;
        freq[x]++;
    }
    for(int i=1;i<=N;i++)
    {
        for(int j=i;j<=N;j+=i) c[i]+=freq[j];
    }
    for(int i=N;i>=1;i--)
    {
        r[i]=c[i]*(c[i]-1)/2;
        for(int j=2*i;j<=N;j+=i) r[i]-=r[j];
    }
    cout<<r[1]<<"\n";
}
