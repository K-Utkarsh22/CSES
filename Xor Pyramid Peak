#include <bits/stdc++.h>
using namespace std;
void solve()
{
    int n;cin>>n;
    vector<long long> v(n);
	for(auto &x:v) cin>>x;
    long long x=0;
    for(int i=0;i<n;i++)
    {
        if(((n-1)&i)==i) x^=v[i];
    }
    cout<<x<<"\n";
}
int main()
{
    int t=1;
    while(t--)
    {
        solve();
    }
}
