#include<bits/stdc++.h>
#define ll long long
using namespace std;
int N=1000000;
vector<ll> freq(N+1);
int main()
{
    ll n;cin>>n;
    for(int i=0;i<n;i++)
    {
        int x;cin>>x;
        freq[x]++;
    }
    ll r=1;
    for(int i=N;i>=1;i--)
    {
        ll c=0;
        for(int j=i;j<=N;j+=i) c+=freq[j];
        if(c>=2)
        {
            r=i;
            break;
        }
    }
    cout<<r<<"\n";
}
