#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long n,k,x,a,b,c;
    cin>>n>>k>>x>>a>>b>>c;
    vector<long long>v(n);
    v[0]=x;
    for(int i=1;i<n;i++)
        v[i]=(a*v[i-1]+b)%c;
    long long s=0;
    for(int i=0;i<k;i++)
        s^=v[i];
    long long r=s;
    for(int i=k;i<n;i++)
    {
        s^=v[i]^v[i-k];
        r^=s;
    }
    cout<<r;
}
