#include<bits/stdc++.h>
using namespace std;
bool chk(long long n)
{
    for(long long i=3;i<=(n/i);i+=2)
    {
        if(n%i==0) return true;
    }
    return false;
}
int main()
{
    int t;cin>>t;
    while(t--)
    {
        long long n;cin>>n;
        if(n==1)
        {
            cout<<2<<"\n";
            continue;
        }
        else if(n&1) n+=2;
        else n+=1;
        while(chk(n))
        {
            n+=2;
        }
        cout<<n<<"\n";
    }
}
