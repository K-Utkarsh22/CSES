#include <bits/stdc++.h>
using namespace std;
int main()
{
    long long n;cin>>n;
    vector<long long> v(n);
    for(auto &x:v) cin>>x;
    map<long long,long long> mp;
    long long m=1,l=0;
    for(long long i=0;i<n;i++)
    {
        if(mp.count(v[i]) && mp[v[i]]>=l)
        {
            l=mp[v[i]]+1;
        }
        mp[v[i]]=i;
        m=max(m,i-l+1);
    }
    cout<<m<<"\n";
