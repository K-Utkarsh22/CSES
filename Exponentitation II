#include <bits/stdc++.h>
using namespace std;
long long mod=10e8+7;
long long solve(long long a,long long b,long long c)
{
    long long f=1;
    while(b>0)
    {
        if(b%2==0)
        {
            a=(a*a)%c;
            b=b/2;
        }
        else
        {
            f=(f*a)%c;
            b--;
        }
    }
    return f;
}
int main()
{
    int n;cin>>n;
    while(n--)
    {
        long long a,b,c;
        cin>>a>>b>>c;
        long long f=solve(b,c,mod-1);
        cout<<solve(a,f,mod)<<"\n";
    }
}
