#include <bits/stdc++.h>
using namespace std;
int main()
{
    long long n,q;cin>>n>>q;
    vector<vector<long long>> v(n+1,vector<long long>(n+1,0));
    for(int j=0;j<n;j++)
    {
        for(int i=0;i<n;i++)
        {
            char x;cin>>x;
            int t=0;if(x=='*') t=1;
            v[i+1][j+1]=v[i][j+1]+v[i+1][j]-v[i][j]+t;
        }
    }
    // for(auto &x:v)
    // {
    //     for(auto &k:x) cout<<k<<" ";
    //     cout<<"\n";
    // }
    while(q--)
    {
        long long x1,y1,x2,y2;cin>>y1>>x1>>y2>>x2;
        //cout<<"v[x2][y2] "<<v[x2][y2]<<"\nv[x1-1][y1-1] "<<v[x1-1][y1-1]<<"\nv[x1-1][y2] "<<v[x1-1][y2]<<"\nv[x2][y1-1] "<<v[x2][y1-1]<<"\n";
        cout<<(v[x2][y2]+v[x1-1][y1-1]-v[x1-1][y2]-v[x2][y1-1])<<"\n";
    }
}
