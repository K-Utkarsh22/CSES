#include <bits/stdc++.h>
#define ll long long
using namespace std;
int main()
{
    int n,k; cin>>n>>k;
    vector<pair<int,int>> v(n);
    for(int i=0;i<n;i++) cin>>v[i].second>>v[i].first;
    sort(v.begin(),v.end());
    multiset<int> s;
    for(int i=0;i<k;i++) s.insert(0);    
    int r=0;
    for(int i=0;i<n;i++)
    {
        auto it=s.upper_bound(v[i].second);
        if(it!=s.begin())
        {
            it--;
            s.erase(it);
            s.insert(v[i].first);
            r++;
        }
    }
    cout<<r<<"\n";
}
