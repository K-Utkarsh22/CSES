#include <bits/stdc++.h>
#define ll long long 
using namespace std;
int main()
{
    ll n,k;cin>>n>>k;
    ll x,a,b,c;cin>>x>>a>>b>>c;
    vector<ll> v(n+1);
    v[1]=x;
    for(int i=2;i<=n;i++) v[i]=(a*v[i-1]+b)%c;
    vector<ll> p(n+1),s(n+1);
    ll t=0;
    for(int i=1;i<=n;i++)
    {
        t|=v[i];
        p[i]=t;
        if(i%k==0) t=0;
    }
    t=0;
    for(int i=n;i>=1;i--)
    {
        t|=v[i];
        s[i]=t;
        if((i-1)%k==0) t=0;
    }
    ll r=0;
    for(int i=k;i<=n;i++) r^=(p[i]|s[i-k+1]);
    cout<<r<<"\n";
}
