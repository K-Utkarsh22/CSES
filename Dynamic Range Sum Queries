#include <bits/stdc++.h>
#define ll long long 
using namespace std;
void updateSegTree(vector<ll> &v,int x,int l,int r,int p,ll n)
{
    if(l==r) v[x]=n;
    else
    {
        int m=(l+r)/2;
        if(p<=m)
            updateSegTree(v,2*x,l,m,p,n);
        else
            updateSegTree(v,2*x+1,m+1,r,p,n);
        v[x]=min(v[2*x],v[2*x+1]);
    }
}
ll segTree(vector<ll> &v,int x,int l,int r,int ql,int qr)
{
    if(ql>r||qr<l)
        return 0;
    if(ql<=l&&r<=qr)
        return v[x];
    int m=(l+r)/2;
    return min(segTree(v,2*x,l,m,ql,qr),segTree(v,2*x+1,m+1,r,ql,qr));
}
int main()
{
    int n,q;cin>>n>>q;
    vector<ll> v(4*n,0);
    for(int i=1;i<=n;++i)
    {
        ll x;
        cin>>x;
        updateSegTree(v,1,1,n,i,x);
    }
    while(q--)
    {
        int t;cin>>t;
        if(t==1)
        {
            int p;
            ll x;
            cin>>p>>x;
            updateSegTree(v,1,1,n,p,x);
        }
        else
        {
            int a,b;
            cin>>a>>b;
            cout<<segTree(v,1,1,n,a,b)<<"\n";
        }
    }
}
