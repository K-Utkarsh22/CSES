#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;cin>>n;
    multiset<long long> s;
    for(int i=0;i<n;i++)
    {
        long long k;cin>>k;
        if(!s.empty() && s.upper_bound(k)!=s.end())
        {
            s.erase(s.upper_bound(k));
        }
        s.insert(k);
    }
    cout<<s.size()<<"\n";

}
